  "author": "Bart Nowak <bnowak@bnowak.com> (http://bnowak.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bartlomn/react-viewstack/issues"
  },
  "homepage": "https://github.com/bartlomn/react-viewstack#readme",
