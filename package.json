{
  "name": "@bnowak/react-viewstack",
  "version": "0.0.1",
  "description": "Simple viewstack with support for animated transitions. Alternative to ReactTransitionGroup with better control over in/out animation direction and transitions effects out of the box.",
  "main": "dist/index.js",
  "scripts": {
    "build": "better-npm-run compile",
    "dev": "better-npm-run develop",
    "lint": "./node_modules/eslint/bin/eslint.js . || true",
    "lint:fix": "./node_modules/eslint/bin/eslint.js . -- --fix || true",
    "test": "npm run lint && npm run testonly"
  },
  "betterScripts": {
    "compile": {
      "command": "./node_modules/del-cli/cli.js 'dist/*.*' && ./node_modules/webpack/bin/webpack.js --config webpack.config.production.js --progress",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "develop": {
      "command": "start-storybook -p 9001 && ./node_modules/opn-cli/cli.js 'http://localhost:9001'",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bartlomn/react-viewstack.git"
  },
  "keywords": [
    "viewstack",
    "view",
    "stack",
    "animated",
    "transitions",
    "effects",
    "container"
  ],
  "author": "Bart Nowak <bnowak@bnowak.com> (http://bnowak.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bartlomn/react-viewstack/issues"
  },
  "homepage": "https://github.com/bartlomn/react-viewstack#readme",
  "devDependencies": {
    "@kadira/storybook": "^1.25.0",
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "css-modules-require-hook": "^4.0.0",
    "del-cli": "^0.2.0",
    "eslint": "^2.9.0",
    "eslint-config-standard": "^5.3.1",
    "eslint-config-standard-react": "^2.4.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-react": "^5.1.1",
    "eslint-plugin-standard": "^1.3.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "postcss": "^5.0.21",
    "postcss-loader": "^0.9.1",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.0.0",
    "webpack": "^1.13.0"
  },
  "peerDependencies": {
    "react": "^0.14.7 || ^15.0.0"
  },
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "classnames": "^2.2.5"
  },
  "devEngines": {
    "node": "4.x || 5.x",
    "npm": "2.x || 3.x"
  }
}
