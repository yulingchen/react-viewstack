{
  "name": "@bnowak/react-viewstack",
  "version": "0.0.1",
  "description": "Simple viewstack with support for animated transitions. Alternative to ReactTransitionGroup with better control over in/out animation direction and transitions effects out of the box.",
  "main": "dist/index.js",
  "scripts": {
    "build": "better-npm-run build",
    "clean": "better-npm-run clean",
    "compile": "better-npm-run compile",
    "coverage:report": "better-npm-run coverage:report",
    "dev": "better-npm-run develop",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix || true",
    "lint:css": "./node_modules/stylelint/dist/cli.js './src/**/*.scss' --syntax scss",
    "lint:css:fix": "./node_modules/stylefmt/bin/cli.js ./src/view-stack.scss",
    "test": "better-npm-run test",
    "test:watch": "better-npm-run test:watch"
  },
  "betterScripts": {
    "compile": {
      "command": "./node_modules/webpack/bin/webpack.js --config webpack.config.production.js --progress",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "clean": {
      "command": "./node_modules/del-cli/cli.js 'dist/**'"
    },
    "build": {
      "command": "npm run clean && npm run lint && npm test && npm run compile",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "develop": {
      "command": "start-storybook -p 9001",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "coverage:report": {
      "command": "./node_modules/ecstatic/lib/ecstatic.js dist/reports/coverage/report-html --port 9002",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "test": {
      "command": "./node_modules/karma/bin/karma start --single-run --no-auto-watch karma.config.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "test:watch": {
      "command": "./node_modules/karma/bin/karma start --auto-watch --no-single-run karma.config.js",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bartlomn/react-viewstack.git"
  },
  "keywords": [
    "viewstack",
    "view",
    "stack",
    "animated",
    "transitions",
    "effects",
    "container",
    "fullpage"
  ],
  "author": "Bart Nowak <bnowak@bnowak.com> (http://bnowak.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bartlomn/react-viewstack/issues"
  },
  "homepage": "https://github.com/bartlomn/react-viewstack#readme",
  "devDependencies": {
    "@kadira/storybook": "^1.25.0",
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "better-npm-run": "0.0.8",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.4.2",
    "cheerio": "^0.20.0",
    "css-loader": "^0.23.1",
    "css-modules-require-hook": "^4.0.0",
    "del-cli": "^0.2.0",
    "ecstatic": "^1.4.1",
    "enzyme": "^2.3.0",
    "eslint": "^2.10.2",
    "eslint-config-standard": "^5.3.1",
    "eslint-config-standard-react": "^2.4.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-react": "^5.1.1",
    "eslint-plugin-standard": "^1.3.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "isparta-loader": "^2.0.0",
    "karma": "^0.13.22",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.0.0",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.0.3",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sinon": "^1.0.5",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.5",
    "node-sass": "^3.7.0",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "^2.1.7",
    "postcss": "^5.0.21",
    "postcss-loader": "^0.9.1",
    "react": "^15.0.2",
    "react-addons-test-utils": "^15.0.2",
    "react-dom": "^15.0.2",
    "sass-loader": "^3.2.0",
    "sinon": "^1.17.4",
    "style-loader": "^0.13.1",
    "stylefmt": "^3.2.1",
    "stylelint": "^6.4.1",
    "stylelint-config-standard": "^7.0.0",
    "webpack": "^1.13.0"
  },
  "peerDependencies": {
    "react": "^0.14.7 || ^15.0.2"
  },
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "classnames": "^2.2.5"
  },
  "devEngines": {
    "node": "4.x || 5.x",
    "npm": "2.x || 3.x"
  }
}
